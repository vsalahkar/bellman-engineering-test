{
  "env": {
    "es6": true,
    "node": true,
    "mocha": true,
    "jest": true,
    "browser": true
  },
  "extends": [
    "plugin:vue/essential",
  ],
  "plugins": [
    "vue"
  ],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "globals": {
    "rootRequire": true,
    "log": true
  },
  "rules": {
    "vue/comment-directive": 0,
    "no-var": [
      2
    ],
    "brace-style": [
      2,
      "1tbs",
      {
        "allowSingleLine": true
      }
    ],
    "camelcase": [
      2,
      {
        "properties": "always"
      }
    ],
    "comma-dangle": [
      1,
      "only-multiline"
    ],
    "comma-spacing": [
      2,
      {
        "before": false,
        "after": true
      }
    ],
    "comma-style": 2,
    "complexity": [
      2,
      8
    ],
    "constructor-super": 0,
    "curly": 2,
    "dot-notation": 2,
    "eol-last": 0,
    "eqeqeq": 2,
    "generator-star-spacing": 2,
    "id-length": [
      2,
      {
        "min": 2,
        "properties": "always"
      }
    ],
    "indent": [
      2,
      4,
      {
        "SwitchCase": 1,
        "VariableDeclarator": {
          "var": 2,
          "let": 1,
          "const": 1
        },
        "MemberExpression": 1,
        "FunctionDeclaration": {
          "parameters": "first"
        },
        "ArrayExpression": 1,
        "ObjectExpression": 1
      }
    ],
    "key-spacing": 2,
    "keyword-spacing": 2,
    "linebreak-style": [
      2,
      "unix"
    ],
    "max-depth": [
      2,
      5
    ],
    "max-params": [
      2,
      5
    ],
    "max-statements": [
      2,
      50
    ],
    "new-cap": [
      1,
      {
        "capIsNewExceptions": [
          "AnalyticsLogTiming",
          "Router",
          "ObjectId"
        ]
      }
    ],
    "no-alert": 2,
    "no-cond-assign": [
      2,
      "always"
    ],
    "no-console": [
      2,
      {
        "allow": [
          "info",
          "warn",
          "error"
        ]
      }
    ],
    "no-const-assign": 2,
    "no-confusing-arrow": 2,
    "no-duplicate-imports": 2,
    "no-else-return": 1,
    "no-ex-assign": 2,
    "no-extra-semi": 2,
    "no-iterator": 1,
    "no-labels": 0,
    "no-multi-spaces": 2,
    "no-mixed-operators": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multi-assign": 2,
    "no-multi-str": 2,
    "no-nested-ternary": 2,
    "no-octal": 2,
    "no-plusplus": 2,
    "no-shadow": 2,
    "no-sparse-arrays": 2,
    "no-this-before-super": 0,
    "no-undef": 2,
    "no-underscore-dangle": [
      2,
      {
        "allow": [
          "_id",
          "_doc"
        ]
      }
    ],
    "no-unneeded-ternary": 2,
    "no-unreachable": 2,
    "nonblock-statement-body-position": 2,
    "object-curly-spacing": [
      1,
      "always"
    ],
    "object-shorthand": [
      "error",
      "always"
    ],
    "one-var": [
      2,
      "never"
    ],
    "prefer-const": 2,
    "prefer-template": 1,
    "prefer-arrow-callback": 1,
    "quotes": [
      2,
      "single"
    ],
    "radix": 2,
    "require-yield": 2,
    "semi": [
      2,
      "never"
    ],
    "space-before-blocks": 2,
    "spaced-comment": 1,
    "space-infix-ops": 1,
    "valid-typeof": 2,
    "vars-on-top": 2,
    "yoda": [
      0,
      "never"
    ]
  }
}
